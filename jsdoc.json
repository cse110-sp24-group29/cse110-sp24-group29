{
  "source": {
    "include": ["source/scripts"],  
    "exclude": ["node_modules"],  
    "includePattern": ".+\\.js$", 
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "plugins": ["plugins/markdown"],
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true
  },
  "opts": {
    "destination": "./docs/",
    "recurse": true,
    "readme": "./README.md"
  }
}
